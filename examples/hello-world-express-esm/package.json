{
  "name": "hello-world-express-esm",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "doit": "npm run clean && npm run build && OTEL_SERVICE_NAME=hello-node-express-ts node --experimental-loader=import-in-the-middle/hook.mjs ./dist/index.js",
    "clear": "rm -rf dist node_modules package-lock.json",
    "start": "OTEL_SERVICE_NAME=hello-node-express-ts node ./dist/index.js",
    "start-esm": "OTEL_SERVICE_NAME=hello-node-express-ts node --experimental-loader=import-in-the-middle/hook.mjs ./dist/index.js"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.3.0",
    "@opentelemetry/instrumentation": "^0.34.0",
    "@opentelemetry/instrumentation-express": "^0.32.0",
    "@opentelemetry/instrumentation-http": "^0.34.0",
    "@opentelemetry/sdk-trace-node": "^1.8.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
